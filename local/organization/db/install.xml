<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/organization/db" VERSION="20190511" COMMENT="XMLDB file for Moodle mod/organization"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="organization" COMMENT="Default comment for organization, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" />
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Name of the organization" />
        <FIELD NAME="email" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="Email id" />
        <FIELD NAME="phone" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="Phone Number" />
        <FIELD NAME="city" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="City" />
        <FIELD NAME="street" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="Street" />
        <FIELD NAME="country" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="Country" />
        <FIELD NAME="state" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="State" />
        <FIELD NAME="zip" TYPE="char" LENGTH="255" NOTNULL="true"  SEQUENCE="false" COMMENT="Zip Code" />
        <FIELD NAME="logo" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="Logo" />
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
